{
  "name"         : "garden",
  "version"      : "0.0.1",
  "description"  : "To analize data from garden",
  "main"         : "server.js",
  "author"       : "peryi",
  "dependencies" : {
    "express"    : "~4.7.2",
    "mongoose"   : "~3.6.2",
    "morgan"     : "~1.2.2",
    "body-parser": "~1.5.2",
    "method-override": "~2.1.2",
    "async": "~0.9.0",
    "winston": "~1.0.0",
    "express-validator": "~2.9.1",
    "util": "~0.10.3",
    "imagemagick": "~0.1.3",
    "multer": "~0.1.8",
    "cors": "~2.7.1",
    "lodash": "~4.6.1",
    "extfs": "0.0.7"
  },
  "devDependencies": {
    "mocha": "1.20.1",
    "should": "4.0.4",
    "nock": "0.42.3",
    "istanbul": "0.3.13",
    "node-inspector": "0.9.2",
    "jshint": "2.5.10",
    "opener": "1.4.0",
